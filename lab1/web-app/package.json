{
  "name": "workmate-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.7.1",
    "boostrap": "^2.0.0",
    "chart.js": "^2.9.4",
    "chartjs-plugin-annotation": "^0.5.7",
    "chartjs-plugin-deferred": "^1.0.1",
    "firebase": "^8.2.9",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.0",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "react-player": "^2.9.0",
    "react-redux": "^7.2.2",
